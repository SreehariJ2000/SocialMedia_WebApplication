@{
    ViewBag.Title = "Contact";
}
<!--  css -->
<link href="~/Content/CustomCSS/HomeCSS/Contactus.css" rel="stylesheet" />


<div class="contact-us-section">
    <div class="contact-form">
        <h2>Contact Us</h2>
        <form>
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" required>

            <label for="description">Description:</label>
            <textarea id="description" name="description" rows="5" required></textarea>

            <button type="submit">Submit</button>
        </form>
    </div>

    <div class="location-map">
        <h2>Our Location</h2>
        <div class="map-container">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3806.7355635931564!2d78.4746609153717!3d17.385044088055333!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bcb973ed9e1c9f7%3A0xa599b8e02a6340f3!2sHyderabad%2C%20Telangana%2C%20India!5e0!3m2!1sen!2sus!4v1667819462931!5m2!1sen!2sus"
                    width="100%" height="300" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
        <address>
            <p>1234 Street Name</p>
            <p>Hyderabad, Telangana, India</p>
            <p>+1 (123) 456-7890</p>
        </address>
    </div>
</div>
